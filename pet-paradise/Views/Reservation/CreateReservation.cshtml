@model pet_paradise.Models.Reservation

<h2>You have created a new reservation!</h2>
<p>for: @Model.PetName</p>
<p>specie: @Model.Specie</p>
<p>customer name: @Model.Customer.Firstname</p>
<p>employee name: @Model.Employee.Firstname</p>
<p>Arrival date: @Model.StartDate.ToString("dd-MM-yyyy")</p>
<p>Departure date: @Model.EndDate.ToString("dd-MM-yyyy")</p>
<a href="@Url.Action("Index", "Reservation")">Go back</a>